<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>DVDpro.StarUML.FileFormat</RootNamespace>
    <AssemblyName>DVDpro.StarUML.FileFormat</AssemblyName>
    <Version>0.1.2</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Read and write StarUML file format out of StartUML tool.</Description>
    <RepositoryType>GIT</RepositoryType>
    <RepositoryUrl>https://github.com/DVDpro/StarUML-fileformat.git</RepositoryUrl>
    <PackageProjectUrl>https://github.com/DVDpro/StarUML-fileformat</PackageProjectUrl>
    <PackageReleaseNotes>tags node and basic generator support</PackageReleaseNotes>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="4.6.0" />
  </ItemGroup>

</Project>
